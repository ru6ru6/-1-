    "\n",
    "PI = 3.1415\n",
    "\n",
    "def rectangle ( w , l ) :\n",
    "    return w * l\n",
    "\n",
    "def triangle ( s , h ) :\n",
    "    return s * h / 2\n",
    "\n",
    "def square ( s ) :\n",
    "    return s ** 2\n",
    "\n",
    "def circle ( r ) :\n",
    "    return PI * r ** 2"
